 "html": "<!DOCTYPE HTML>\n{% if letter_head -%}\n    <div class=\"letter-head\">{{ letter_head }}</div>\n{%- endif %}\n<div style=\"width:100%\">\n    <table class = \"table table-bordered\" style=\"width:100%\">\n        <tr>\n            <td style=\"width:25%\">\n                <b>BOM No</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.bom_no }}\n            </td>\n            <td style=\"width:25%\">\n                <b>Expected Yeild</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.bom_yeild }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>BOM Revision Date</b>\n            </td>\n            <td style=\"width:25%\">\n                \n            </td>\n            <td style=\"width:25%\">\n                <b>Actual Yeail</b>\n            </td>\n            <td style=\"width:25%\">\n                 {{ doc.yeild }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>Item Code</b>\n            </td>\n            <td style=\"width:25%\">\n                 {{ doc.production_item }}\n            </td>\n            <td style=\"width:25%\">\n                <b>Qty To Manufacture</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.qty }} \n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>Item Name</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.item_name }}\n            </td>\n            <td style=\"width:25%\">\n                <b> Manufactured Qty</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.produced_qty }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>Word Order No. </b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.name }}\n            </td>\n            <td style=\"width:25%\">\n                <b>RM Cost Center</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.rm_cost_center }}\n            </td>\n        </tr>\n    </table>\n    \n    <table class = \"table table-bordered\" style=\"width:100%\">\n        <tr>\n            <td style=\"width:100%; text-align:center\" colspan=\"4\">\n                <b>WO Weight Information</b>\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>Expected FG Weight </b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.bom_weight }}\n            </td>\n            <td style=\"width:25%\">\n                <b>RM Weight</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.rm_weight }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>Actual FG Weright</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.fg_weight }}\n            </td>\n            <td style=\"width:25%\">\n                <b>Issue Material Against</b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.transfer_material_against }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:25%\">\n                <b>BOM Weight </b>\n            </td>\n            <td style=\"width:25%\">\n                {{ doc.bom_weight }}\n            </td>\n            <td style=\"width:25%\">\n            </td>\n            <td style=\"width:25%\">\n            </td>\n        </tr>\n    </table>\n    {%- if doc.quality_inspection_par -%}\n    <table class = \"table table-bordered\" style=\"width:100%\">\n        <tr>\n            <td style=\"width:100%; text-align:center\" colspan=\"5\">\n                <b>InProcess Quality Information</b>\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:8%;\"><b>Sr No.</b></td>\n            <td style=\"width:23%;\"><b>Inspection Name</b></td>\n            <td style=\"width:23%;\"><b>Inspection ID</b></td>\n            <td style=\"width:23%;\"><b>Inspection Type</b></td>\n            <td style=\"width:23%;\"><b>Applicable on</b></td>\n        </tr>\n        {%- for row in doc.quality_inspection_par -%}\n            <tr>\n                <td style=\"width:8%;\">{{ row.idx}}</td>\n                <td style=\"width:23%;\">{{ row.inspection_name}}</td>\n                <td style=\"width:23%;\">{{ row.inprocess_quality_inspection_template}}</td>\n                <td style=\"width:23%;\">{{ row.inspection_type}}</td>\n                <td style=\"width:23%;\">{{ row.inspection_applicable_on}}</td>\n            </tr>\n        {%- endfor %}\n    </table>\n    {%- endif %}\n    \n    {%- if doc.operations -%}\n    <table class = \"table table-bordered\" style=\"width:100%\">\n        <tr>\n            <td style=\"width:100%; text-align:center\" colspan=\"10\">\n                <b>Job Card Information</b>\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:10%; font-size:10px\"><b>Name</b></td>\n            <td style=\"width:10%; font-size:10px\"><b>Workstation</b></td>\n            <td style=\"width:10%; font-size:10px\"><b>Operation</b></td>\n            <td style=\"width:10%; font-size:10px\"><b>Employee</b></td>\n            <td style=\"width:10%; font-size:10px\"><b>Status</b></td>\n            <td style=\"width:12%; font-size:10px\"><b>Expected Start</b></td>\n            <td style=\"width:12%; font-size:10px\"><b>Expected End</b></td>\n            <td style=\"width:8%; font-size:10px\"><b>Total Time</b></td>\n            <td style=\"width:10%; font-size:10px\"><b>Expected Cost</b></td>\n            <td style=\"width:8%; font-size:10px\"><b>Actual Cost</b></td>\n        </tr>\n        {%- for row in doc.operations -%}\n            {% set data = get_job_card_name(row) %}\n            <tr>\n                <td style=\"width:8%; font-size:10px\">{{ data[0] }}</td>\n            \t<td style=\"width:10%; font-size:10px\">{{ row.workstation }}</td>\n            \t<td style=\"width:10%; font-size:10px\">{{ row.operation }}</td>\n            \t<td style=\"width:10%; font-size:10px\">{{ data[1] }}</td>\n            \t<td style=\"width:10%; font-size:10px\">{{ row.status }}</td>\n            \t<td style=\"width:12%; font-size:10px\">{{ row.get_formatted(\"planned_start_time\") }}</td>\n            \t<td style=\"width:12%; font-size:10px\">{{ row.get_formatted(\"planned_end_time\") }}</td>\n            \t<td style=\"width:8%; font-size:10px\">{{ row.get_formatted(\"time_in_mins\") }}</td>\n            \t<td style=\"width:10%; font-size:10px\">{{ row.planned_operating_cost }}</td>\n            \t<td style=\"width:8%; font-size:10px\">{{ row.actual_operating_cost }}</td>\n            </tr>\n        {%- endfor %}\n    </table>\n    {%- endif %}\n    \n    <table class = \"table table-bordered\" style=\"width:100%\">\n        <tr>\n            <td style=\"width:100%; text-align:center\" colspan=\"10\">\n                <b>Material Information</b>\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width:8%; font-size:10px;\"><b>Sr No.</b></td>\n            <td style=\"width:11%; font-size:10px;\"><b>Item Code</b></td>\n            <td style=\"width:11%; font-size:10px;\"><b>Item Name</b></td>\n            <td style=\"width:10%; font-size:10px;\"><b>Expected Qty</b></td>\n            <td style=\"width:10%; font-size:10px;\"><b>Issues Qty</b></td>\n            <td style=\"width:10%; font-size:10px;\"><b>Consume Qty</b></td>\n            <td style=\"width:8%; font-size:10px;\"><b>Rate</b></td>\n            <td style=\"width:12%; font-size:10px;\"><b>Amount</b></td>\n            <td style=\"width:10%; font-size:10px;\"><b>Type</b></td>\n            <td style=\"width:10%; font-size:10px;\"><b>Is Add. Item</b></td>\n        </tr>\n        {%- for row in doc.required_items -%}\n            <tr>\n                <td style=\"width:8%; font-size:10px;\">{{ row.idx }}</td>\n                <td style=\"width:12%; font-size:10px;\">{{ row.item_code }}</td>\n                <td style=\"width:12%; font-size:10px;\">{{ row.item_name }}</td>\n                <td style=\"width:10%; font-size:10px;\">{{ row.get_formatted(\"required_qty\") }}</td>\n                <td style=\"width:10%; font-size:10px;\">{{ row.get_formatted(\"transferred_qty\") }}</td>\n                <td style=\"width:10%; font-size:10px;\">{{ row.get_formatted(\"consumed_qty\") }}</td>\n                <td style=\"width:8%; font-size:10px;\">{{ row.get_formatted(\"rate\") }} </td>\n                <td style=\"width:12%; font-size:10px;\">{{ row.get_formatted(\"amount\") }}</td>\n                <td style=\"width:8%; font-size:10px;\"></td>\n                <td style=\"width:8%; font-size:10px;\">\n                    {% if row.additional_material -%}\n                        Yes\n                    {% else -%}\n                        NO\n                    {%- endif %}\n                </td>\n            </tr>\n        {%- endfor %}\n    </table>\n    \n</div>\n",
